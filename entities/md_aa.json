{
    "uri": "http://ontotrans.eu/meta/1.0/md_aa",
    "description": "LAMMPS log file with the thermodynamical output of an atomistic molecular dynamic simulation.",
    "dimensions": {
        "n": "Number of data points."
    },
    "properties": {
        "Step" : {
            "type" : "int",
            "description": "Number of steps used to integrate the equation of motions in a MD simulation.",
            "shape": ["n"]
        },
        "TotEng" : { 
            "type": "float64",
            "description": "Total Energy",
            "units" : "Kcal/mol",
            "shape": ["n"]
        },
        "E_vdwl" : {
            "type": "float64",
            "description": "van der Waals pairwise energy.",
            "units" : "Kcal/mol",
            "shape": ["n"]
        },
        "E_coul" : {
            "type": "float64",
            "description": "Real-space Coulomb pairwise energy.",
            "units" : "Kcal/mol",
            "shape": ["n"]
        },
        "E_long" : {
            "type": "float64",
            "description": "Reciprocal-space Coulomb energy.",
            "units" : "kJ/mol",
            "shape": ["n"]
        },
        "E_bond" : {
            "type": "float64",
            "description": "Bond energy.",
            "units" : "kcal/mol",
            "shape": ["n"]

        },
        "E_angle" : {
            "type": "float64",
            "description": "Angle energy.",
            "units" : "kcal/mol",
            "shape": ["n"]

        },
        "E_dihed" : {
            "type": "float64",
            "description": "Dihedral angle energy.",
            "units" : "kcal/mol",
            "shape": ["n"]
        },
        "E_impro" : {
            "type": "float64",
            "description": "Improper angle energy.",
            "units" : "kcal/mol",
            "shape": ["n"]
        },
        "KinEng" : {
            "type" : "float64",
            "description": "Kinetic energy.",
            "units" : "kcal/mol",
            "shape": ["n"]
        },
        "PotEng" : {
            "type": "float64",
            "description": "Potential energy.",
            "units" : "kcal/mol",
            "shape": ["n"]
        },
        "Temp" : {
            "type": "float64",
            "description": "Temperature",
            "units" : "K",
            "shape": ["n"]
        },
        "Press" : {
            "type": "float64",
            "description": "Pressure",
            "units" : "atm",
            "shape": ["n"]
        },
        "Volume" : {
            "type": "float64",
            "description": "Simulation cell volume.",
            "units" : "Angs3",
            "shape": ["n"]
        },
        "Density" : {
            "type": "float64",
            "description": "Mass density.",
            "units" : "g/cm3",
            "shape": ["n"]
        },
        "c_inter" : {
            "type": "float64",
            "description": "Intermolecular Energy.",
            "units" : "Kcal/mol",
            "shape": ["n"]
        },
        "v_msqdis" : {
            "type": "float64",
            "description": "Molecular-averaged mean-square displacement.",
            "units" : "Angs2",
            "shape": ["n"]
        },
        "CPU" : {
            "type": "float64",
            "description": "Elapsed CPU time since start of the run.",
            "units" : "s",
            "shape": ["n"]
        }
    }
}
